{
  "name": "ply-web",
  "version": "0.0.1",
  "scripts": {
    "build": "npm run monaco-workers && npm run fonts && rollup -c",
    "watch": "rollup --watch -c",
    "fonts": "cp -R fonts out",
    "editor-worker": "parcel build node_modules/monaco-editor/esm/vs/editor/editor.worker.js --dist-dir src/workers",
    "json-worker": "parcel build node_modules/monaco-editor/esm/vs/language/json/json.worker.js --dist-dir src/workers",
    "monaco-workers": "npm run editor-worker && npm run json-worker"
  },
  "dependencies": {
    "element-plus": "latest",
    "highlight.js": "^11.1.0",
    "monaco-editor": "^0.25.2",
    "vue": "^3.1.4"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@rollup/plugin-typescript": "^8.2.1",
    "@vue/compiler-sfc": "^3.1.4",
    "eslint": "^7.27.0",
    "eslint-plugin-vue": "^7.13.0",
    "node-sass": "^6.0.1",
    "parcel": "^2.0.0-beta.3.1",
    "rollup": "^2.53.1",
    "rollup-plugin-copy-watch": "^0.0.1",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "^4.2.4"
  }
}
